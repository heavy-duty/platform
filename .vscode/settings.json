{
  "conventionalCommits.scopes": [
    "core",
    "bulldozer-programs",
    "bulldozer-client",
    "editor",
    "wallet-adapter",
    "ngx-solana",
    "ngx-websocket"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust"
  }
}
