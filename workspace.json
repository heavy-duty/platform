{
  "version": 2,
  "projects": {
    "bulldozer": {
      "root": "apps/bulldozer",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@danmt/nx-anchor:build",
          "options": {
            "projectPath": "apps/bulldozer",
            "monitor": true
          }
        },
        "test": {
          "executor": "@danmt/nx-anchor:test",
          "options": {
            "projectPath": "apps/bulldozer",
            "monitor": true
          }
        },
        "deploy": {
          "executor": "@danmt/nx-anchor:deploy",
          "options": {
            "projectPath": "apps/bulldozer",
            "monitor": true
          }
        }
      }
    },
    "bulldozer-application-features-connect-wallet": {
      "projectType": "library",
      "root": "libs/bulldozer/application/features/connect-wallet",
      "sourceRoot": "libs/bulldozer/application/features/connect-wallet/src",
      "prefix": "bd",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/bulldozer/application/features/connect-wallet"
          ],
          "options": {
            "jestConfig": "libs/bulldozer/application/features/connect-wallet/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/bulldozer/application/features/connect-wallet/src/**/*.ts",
              "libs/bulldozer/application/features/connect-wallet/src/**/*.html"
            ]
          }
        }
      }
    },
    "bulldozer-application-features-navigation": {
      "projectType": "library",
      "root": "libs/bulldozer/application/features/navigation",
      "sourceRoot": "libs/bulldozer/application/features/navigation/src",
      "prefix": "bd",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/bulldozer/application/features/navigation"
          ],
          "options": {
            "jestConfig": "libs/bulldozer/application/features/navigation/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/bulldozer/application/features/navigation/src/**/*.ts",
              "libs/bulldozer/application/features/navigation/src/**/*.html"
            ]
          }
        }
      }
    },
    "bulldozer-application-shell": {
      "projectType": "library",
      "root": "libs/bulldozer/application/shell",
      "sourceRoot": "libs/bulldozer/application/shell/src",
      "prefix": "bd",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/bulldozer/application/shell"],
          "options": {
            "jestConfig": "libs/bulldozer/application/shell/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/bulldozer/application/shell/src/**/*.ts",
              "libs/bulldozer/application/shell/src/**/*.html"
            ]
          }
        }
      }
    },
    "bulldozer-application-utils-pipes-obscure-address": {
      "projectType": "library",
      "root": "libs/bulldozer/application/utils/pipes/obscure-address",
      "sourceRoot": "libs/bulldozer/application/utils/pipes/obscure-address/src",
      "prefix": "heavy-duty",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/bulldozer/application/utils/pipes/obscure-address"
          ],
          "options": {
            "jestConfig": "libs/bulldozer/application/utils/pipes/obscure-address/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/bulldozer/application/utils/pipes/obscure-address/src/**/*.ts",
              "libs/bulldozer/application/utils/pipes/obscure-address/src/**/*.html"
            ]
          }
        }
      }
    },
    "bulldozer-application-utils-pipes-sanitize-url": {
      "projectType": "library",
      "root": "libs/bulldozer/application/utils/pipes/sanitize-url",
      "sourceRoot": "libs/bulldozer/application/utils/pipes/sanitize-url/src",
      "prefix": "heavy-duty",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/bulldozer/application/utils/pipes/sanitize-url"
          ],
          "options": {
            "jestConfig": "libs/bulldozer/application/utils/pipes/sanitize-url/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/bulldozer/application/utils/pipes/sanitize-url/src/**/*.ts",
              "libs/bulldozer/application/utils/pipes/sanitize-url/src/**/*.html"
            ]
          }
        }
      }
    },
    "bulldozer-client": {
      "projectType": "application",
      "root": "apps/bulldozer-client",
      "sourceRoot": "apps/bulldozer-client/src",
      "prefix": "bd",
      "targets": {
        "build": {
          "executor": "@angular-builders/custom-webpack:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/bulldozer-client",
            "index": "apps/bulldozer-client/src/index.html",
            "main": "apps/bulldozer-client/src/main.ts",
            "polyfills": "apps/bulldozer-client/src/polyfills.ts",
            "tsConfig": "apps/bulldozer-client/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/bulldozer-client/src/favicon.ico",
              "apps/bulldozer-client/src/assets"
            ],
            "styles": ["apps/bulldozer-client/src/styles.scss"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "apps/bulldozer-client/webpack.config.js",
              "mergeStrategies": {
                "externals": "replace"
              }
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/bulldozer-client/src/environments/environment.ts",
                  "with": "apps/bulldozer-client/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "bulldozer-client:build:production"
            },
            "development": {
              "browserTarget": "bulldozer-client:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bulldozer-client:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/bulldozer-client/src/**/*.ts",
              "apps/bulldozer-client/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/bulldozer-client"],
          "options": {
            "jestConfig": "apps/bulldozer-client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "bulldozer-client-e2e": {
      "root": "apps/bulldozer-client-e2e",
      "sourceRoot": "apps/bulldozer-client-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/bulldozer-client-e2e/cypress.json",
            "devServerTarget": "bulldozer-client:serve:development",
            "tsConfig": "apps/bulldozer-client-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "bulldozer-client:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/bulldozer-client-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shared-utils-operators": {
      "root": "libs/shared/utils/operators",
      "sourceRoot": "libs/shared/utils/operators/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/shared/utils/operators/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/utils/operators"],
          "options": {
            "jestConfig": "libs/shared/utils/operators/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "bulldozer-client"
}
