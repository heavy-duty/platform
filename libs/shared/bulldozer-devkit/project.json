{
  "root": "libs/shared/bulldozer-devkit",
  "sourceRoot": "libs/shared/bulldozer-devkit/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/bulldozer-devkit",
        "main": "libs/shared/bulldozer-devkit/src/index.ts",
        "tsConfig": "libs/shared/bulldozer-devkit/tsconfig.lib.json",
        "assets": ["libs/shared/bulldozer-devkit/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/bulldozer-devkit/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/bulldozer-devkit"],
      "options": {
        "jestConfig": "libs/shared/bulldozer-devkit/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
